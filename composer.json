{
    "name": "namelesscoder/typo3-repository-client",
    "description": "Client for interacting with the official TYPO3 Extension Repository (TER). Contains API and CLI scripts.",
    "license": "MIT",
    "authors": [
        {
            "name": "Claus Due",
            "email": "claus@namelesscoder.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "NamelessCoder\\TYPO3RepositoryClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NamelessCoder\\TYPO3RepositoryClient\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "require-dev": {
        "mikey179/vfsStream": "~1.3.0",
        "satooshi/php-coveralls": "*",
        "phpunit/phpunit": "^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    },
    "bin": [
        "bin/setversion",
        "bin/upload",
        "bin/rmversion"
    ],
    "config": {
        "bin-dir": "bin"
    }
}
